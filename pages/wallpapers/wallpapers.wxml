<scroll-view style='background-color:#000000'>
  <block wx:for="{{photoList}}">
    <image src="{{item.thumbUrl}}" style='width:100%;background-color:#000000' data-url="{{item.url}}" data-thumb-url='{{item.thumbUrl}}' mode='widthFix' lazy-load='true' bindtap='jumpToInfo' />
  </block>
  <view class="load-more" hidden='{{isEnd}}'>
    <text class='load-more-text'>加载中...</text>
  </view>
</scroll-view>